{
  "name": "@akashic/akashic-cli-export-html",
  "version": "0.1.12",
  "description": "A module to convert your Akashic game to a runnable standalone.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "gulp clean && gulp compile",
    "install:typings": "gulp install:typings",
    "clean:typings": "gulp clean:typings",
    "clean": "gulp clean",
    "start": "npm run build && node bin/run",
    "build": "gulp compile",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tslint --type-check -c tslint.json --project ./tsconfig.json",
    "lint:md": "remark ./*.md --frail --no-stdout --quiet --rc-path ./.remarkrc",
    "test": "gulp test && npm run lint && npm run lint-md"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-export-html": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "templates",
    "package.json"
  ],
  "devDependencies": {
    "@akashic/akashic-engine": "~2.0.0",
    "@types/commander": "~2.3.31",
    "@types/fs-readdir-recursive": "~1.0.0",
    "@types/node": "~8.0.53",
    "@types/uglify-js": "~2.6.29",
    "browserify": "^13.0.0",
    "del": "~1.1.1",
    "gulp": "~3.9.1",
    "gulp-expect-file": "0.0.7",
    "gulp-istanbul": "~0.10.4",
    "gulp-jasmine": "~2.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "~0.5.2",
    "gulp-tslint": "~4.3.5",
    "gulp-uglify": "~2.0.0",
    "gulp-util": "^3.0.7",
    "jasmine": "~2.1.1",
    "jasmine-reporters": "~2.0.4",
    "jasmine-terminal-reporter": "~0.9.1",
    "remark-cli": "~2.0.0",
    "remark-lint": "~5.0.1",
    "mock-fs": "3.12.1",
    "tslint": "~5.9.0",
    "typescript": "~2.6.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "~0.2.6",
    "@types/fs-extra": "^5.0.0",
    "commander": "2.8.1",
    "ect": "^0.5.9",
    "fs-extra": "3.0.1",
    "fs-readdir-recursive": "~0.1.2",
    "uglify-js": "2.6.1"
  },
  "optionalDependencies": {
    "@akashic/game-driver": "~1.0.0",
    "@akashic/pdi-browser": "~1.0.0",
    "@akashic/game-storage": "0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-cli-export-html.git"
  },
  "publishConfig": {
    "@akashic:registry": "http://registry.npmjs.org/"
  }
}
